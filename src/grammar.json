{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gh_actions_expressions",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "word"
          },
          {
            "type": "SYMBOL",
            "name": "not_word"
          }
        ]
      }
    },
    "word": {
      "type": "PATTERN",
      "value": "\\w+"
    },
    "not_word": {
      "type": "PATTERN",
      "value": "\\W+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
