{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gh_actions_expressions",
  "rules": {
    "pair": {
      "type": "SYMBOL",
      "name": "_if_pair"
    },
    "_if_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "if_key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        }
      ]
    },
    "if_key": {
      "type": "STRING",
      "value": "if"
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "boolean": {
      "type": "PATTERN",
      "value": "true|false"
    },
    "null": {
      "type": "PATTERN",
      "value": "null"
    },
    "_dot": {
      "type": "STRING",
      "value": "."
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_int"
        },
        {
          "type": "SYMBOL",
          "name": "_float"
        },
        {
          "type": "SYMBOL",
          "name": "_hex"
        },
        {
          "type": "SYMBOL",
          "name": "_exp"
        }
      ]
    },
    "_int": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "_float": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_int"
        },
        {
          "type": "SYMBOL",
          "name": "_dot"
        },
        {
          "type": "SYMBOL",
          "name": "_int"
        }
      ]
    },
    "_hex": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "0x"
        },
        {
          "type": "PATTERN",
          "value": "[0-9a-f]+"
        }
      ]
    },
    "_exp": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_float"
        },
        {
          "type": "STRING",
          "value": "e"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_int"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_content"
              },
              {
                "type": "SYMBOL",
                "name": "scape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "string_content": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "([^'\\\\\\r\\n]|\\\\(.|\\r?\\n))+"
        }
      }
    },
    "scape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "STRING",
        "value": "''"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
