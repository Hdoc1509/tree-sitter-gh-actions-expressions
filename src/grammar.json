{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "gh_actions_expressions",
  "rules": {
    "pair": {
      "type": "SYMBOL",
      "name": "_if_pair"
    },
    "_if_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "if_key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "delimited_expression"
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        }
      ]
    },
    "if_key": {
      "type": "STRING",
      "value": "if"
    },
    "_evaluation": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "context"
        },
        {
          "type": "SYMBOL",
          "name": "function_call"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_evaluation"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "operator"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "not"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "_evaluation"
              }
            ]
          }
        }
      ]
    },
    "delimited_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "${{"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "not"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "_evaluation"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "operator"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "SYMBOL",
                        "name": "not"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_evaluation"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null": {
      "type": "STRING",
      "value": "null"
    },
    "_dot": {
      "type": "STRING",
      "value": "."
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_int"
        },
        {
          "type": "SYMBOL",
          "name": "_float"
        },
        {
          "type": "SYMBOL",
          "name": "_hex"
        },
        {
          "type": "SYMBOL",
          "name": "_exp"
        }
      ]
    },
    "_int": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "_float": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_int"
        },
        {
          "type": "SYMBOL",
          "name": "_dot"
        },
        {
          "type": "SYMBOL",
          "name": "_int"
        }
      ]
    },
    "_hex": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "0x"
        },
        {
          "type": "PATTERN",
          "value": "[0-9a-f]+"
        }
      ]
    },
    "_exp": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_float"
        },
        {
          "type": "STRING",
          "value": "e"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "-"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_int"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "string_content"
              },
              {
                "type": "SYMBOL",
                "name": "scape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "string_content": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "([^'\\\\\\r\\n]|\\\\(.|\\r?\\n))+"
        }
      }
    },
    "scape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "STRING",
        "value": "''"
      }
    },
    "asterisk": {
      "type": "STRING",
      "value": "*"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[_a-zA-Z][-_a-zA-Z0-9]+"
    },
    "property_deref": {
      "type": "SYMBOL",
      "name": "_dot"
    },
    "context": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "property"
          }
        }
      ]
    },
    "property": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "property_deref"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "identifier"
            },
            {
              "type": "SYMBOL",
              "name": "asterisk"
            }
          ]
        }
      ]
    },
    "function_call": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "function",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "FIELD",
          "name": "arguments",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "arguments"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "arguments": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_evaluation"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "_evaluation"
              }
            ]
          }
        }
      ]
    },
    "not": {
      "type": "STRING",
      "value": "!"
    },
    "operator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "eq"
        },
        {
          "type": "SYMBOL",
          "name": "and"
        },
        {
          "type": "SYMBOL",
          "name": "or"
        },
        {
          "type": "SYMBOL",
          "name": "ne"
        }
      ]
    },
    "eq": {
      "type": "STRING",
      "value": "=="
    },
    "and": {
      "type": "STRING",
      "value": "&&"
    },
    "or": {
      "type": "STRING",
      "value": "||"
    },
    "ne": {
      "type": "STRING",
      "value": "!="
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
